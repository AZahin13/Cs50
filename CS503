//Terms and Conditions apply
#include <stdio.h>
#include <cs50.h>

int main(void)
{
    char c = get_char("Do you agree to the Terms and Conditions?(Y/N)\n");

    if (c == 'y' || c == 'Y')
    {
        printf("Thank you for agreeing\n");
    }
    else if (c == 'n' || c == 'N')
    {
        printf("You have disagreed, we cannot proceed\n");
    }
}
